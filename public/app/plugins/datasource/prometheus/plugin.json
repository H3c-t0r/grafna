      "authType": "{{if .JsonData.azureAuth}}azure{{else}}none{{end}}",
      "tokenAuth": {
        "scopes": ["https://management.azure.com/.default"],
        "params": {
          "azure_auth_type": "{{.JsonData.azureAuthType | orEmpty}}",
          "azure_cloud": "AzureCloud",
          "tenant_id": "{{.JsonData.azureTenantId | orEmpty}}",
