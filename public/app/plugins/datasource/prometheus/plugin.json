      "method": "POST",
      "path": "/rules",
      "reqRole": "Editor",
      "authType": "{{if .JsonData.azureAuth}}azure{{else}}none{{end}}",
      "tokenAuth": {
        "scopes": ["https://management.azure.com/.default"],
